  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Gray>>> Commetns",
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#999999",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Light-Green>>> Strings",
        "scope": [
          "string",
          "string.template",
          "punctuation.definition.string",
          "support.constant.property-value.css"
        ],
        "settings": {
          "foreground": "#99CC77",
          "fontStyle": "italic"
        }
      },
      {
        "name": "Light-purple>>> Numbers & Constants",
        "scope": [
          "constant",
          "constant.numeric",
          "constant.language",
          "constant.character",
          "constant.other",
          "variable.other.constant",
          "punctuation.definition.constant",
          "punctuation.definition.entity.html"
        ],
        "settings": {
          "foreground": "#9988FF"
        }
      },
      /////////////////////////////////////////////////////////////////
      /////////////////////////////////////////////////////////////////
      /////////////////////////////////////////////////////////////////
      {
        "name": "Light-Gray>>> Text & punctuation & operators",
        "scope": [
          "text",
          "punctuation",
          "keyword.operator.assignment",
          "keyword.operator.arithmetic",
          "keyword.operator.relational",
          "keyword.operator.logical",
          "keyword.operator.comparison",
          "keyword.operator.bitwise",
          "keyword.operator.compound.bitwise",
          "keyword.operator.increment",
          "keyword.operator.decrement",
          "keyword.operator.expression",
          "source",
          "support.constant.mathematical-symbols",
          "support.type.property-name.css"
        ],
        "settings": {
          "foreground": "#CCCCCC"
        }
      },
      {
        "name": "Cinnabar>>> Built-in & keywords & storage",
        "scope": [
          "keyword",
          "keyword.control",
          "keyword.operator",
          "keyword.operator.word",
          "punctuation.definition.keyword",
          "storage",
          "storage.type",
          "storage.modifier",
          "entity.name.tag"
        ],
        "settings": {
          "foreground": "#e66533"
        }
      },
      {
        "name": "Yellow>>> Variables",
        "scope": [
          "variable",
          "source.json meta.structure.dictionary.json support.type.property-name.json",
          "entity.other.attribute-name",
          "keyword.other.unit"
        ],
        "settings": {
          "foreground": "#DD9911",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Mehh-Yellow>>> Readwrite-Variables & Parameters",
        "scope": [
          "entity.name.variable",
          "variable.other.readwrite",
          "variable.other.property",
          "variable.other.object.property",
          "variable.other.assignment",
          "variable.parameter",
          "punctuation.definition.variable"
        ],
        "settings": {
          "foreground": "#FF9729",
          "fontStyle": ""
        }
      },
      /////////////////////////////////////////////////////////////////
      /////////////////////////////////////////////////////////////////
      /////////////////////////////////////////////////////////////////
      {
        "name": "CONSTANT",
        "scope": [],
        "settings": {
          "foreground": "#d67e5c"
        }
      },
      {
        "name": "Blue>>> Methods & Function-clalls",
        "scope": ["entity", "support.function.misc.scss"],
        "settings": {
          "foreground": "#44AAEE"
        }
      },
      {
        "name": "MintGreen>>> Support",
        "scope": ["support"],
        "settings": {
          "foreground": "#44EEAA"
        }
      },
      {
        "name": "Pick>>> MISC",
        "scope": ["support.variable.property"],
        "settings": {
          "foreground": "#df769b"
        }
      },
      /////////////////////////////////////////////////////////////////
      /////////////////////////////////////////////////////////////////
      /////////////////////////////////////////////////////////////////
      {
        "name": "INVALID",
        "scope": ["invalid", "invalid.illegal", "invalid.deprecated"],
        "settings": {
          "foreground": "#FF3030",
          "fontStyle": "underline"
        }
      },
      {
        "name": "ITALIC",
        "scope": ["string", "comment", "constant.language"],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "name": "BOLD",
        "scope": ["punctuation", "keyword.operator"],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "name": "BOLD-ITALIC",
        "scope": ["keyword", "storage"],
        "settings": {
          "fontStyle": "bold italic"
        }
      },
      {
        "name": "NORMAL",
        "scope": [
          "source.json meta.structure.dictionary.json support.type.property-name.json"
        ],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "name": "STRIKETHROUGH",
        "scope": ["invalid.deprecated"],
        "settings": {
          "fontStyle": "strikethrough"
        }
      }
